<%= render :partial => 'team_charts', :locals => {:prefix => nil} %>

<TR class="team" <% if prefix %>id='<%= prefix %>_<%= num + 4 %>' style="display:none"<% end %>>
	<TH>&nbsp;</TH>
	<TH COLSPAN="<%= col_outer_th_span_value %>">Submitted work</TH>
	<TH WIDTH="9">&nbsp;</TH>
</TR>

<TR class="team" <% if prefix %>id='<%= prefix %>_<%= num + 5 %>' style="display:none"<% end %>>
	<TH>Team</TH>
	<TH COLSPAN="<%= col_avg_th_span_value %>">Average</TH>
	<TH COLSPAN="<%= col_range_th_span_value %>">Range</TH>
	<TH WIDTH="9">&nbsp;</TH>
</TR>
